{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "api/contact.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/thank-you-new.html",
      "dest": "/public/thank-you-new.html"
    },
    {
      "src": "/thank-you.html",
      "dest": "/public/thank-you.html"
    },
    {
      "src": "/contact.php",
      "dest": "/api/contact.php"
    },
    {
      "src": "/(.*\\.(html|css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}

